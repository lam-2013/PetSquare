<% provide(:title, "Edit user") %>
<h1>Modifica il tuo profile</h1>

<div class="row">
  <div class="small-6 small-centered columns">
    <%= form_for(@user) do |f| %>
        <%= render 'fields', f: f %>
        <%= f.submit 'Salva cambiamenti', class: 'small round primary button' %>
    <% end %>

  </div>
</div>
